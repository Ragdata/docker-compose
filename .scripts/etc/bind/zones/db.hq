;
; BIND zone file for "hq" extension
;
$TTL    604800
@       	IN      SOA     hq. admin.hq. (
					 	 3         ; Serial
					604800         ; Refresh
                     86400         ; Retry
                   2419200         ; Expire
	                604800 )       ; Negative Cache TTL

; name servers - NS records
			IN		NS		ns1.hq.
			IN		NS		ns2.hq.

; name servers - A records
ns1			IN		A		10.0.0.101
ns2			IN		A		10.0.0.102

; 10.0.0.0/16 - A records
gateway		IN		A		10.0.0.1
switch		IN		A		10.0.0.108
pve			IN		A		10.0.0.110
nas			IN		A		10.0.0.111
swarm		IN		A		10.0.0.200
